{
  "hosting": {
    "public": "public",
    "site": "evasim",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
	    {
		    "source": "/terminal",
		    "destination": "/terminal/index.html"
	    },
	    {
		    "source": "**",
		    "destination": "/index.html"
	    }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
